@model Countries

<form method="post" asp-action="delete">
    <div class="p-3 mt-4">
        <div class="row pb-2">
            <h2 class="text-primary">Create Unit</h2>
        </div>
        <div class="mb-3">
            <label asp-for="Name"></label>
            <input asp-for="Name" class="form-control" disabled />
        </div>
        <div class="mb-3">
            <label asp-for="CountryCode"></label>
            <input asp-for="CountryCode" class="form-control" disabled />
        </div>
        <button type="submit" class="btn btn-danger">Delete</button>
        <a asp-controller="Countries" asp-action="Index" class="btn btn-secondary">Back</a>
    </div>
</form>